{
  "name": "vue-techen-ui",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "files": ["dist"], 
  "main": "./dist/vue-techen-ui.umd.cjs",
  "module": "./dist/vue-techen-ui.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    "./css": "./dist/style.css",
    ".": {
      "import": "./dist/vue-techen-ui.js",
      "require": "./dist/vue-techen-ui.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.6.2",
    "element-plus": "^2.4.2",
    "naive-ui": "^2.35.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@smallwei/avue": "^3.2.22",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "typescript": "~5.2.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19",
    "webpack": "^5.0.0"
  },
  "description": "A Vue3 UI Components Library.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenlong890223/vue-techen-ui.git"
  },
  "keywords": [
    "Vue3",
    "TS",
    "Vite",
    "Less",
    "Techen",
    "UI",
    "Components",
    "Tools"
  ],
  "author": "Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chenlong890223/vue-techen-ui/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
